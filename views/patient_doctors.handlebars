<nav>
        <div class="blue lighten-1 nav-wrapper">
            <a class="brand-logo center">One Stop Health</a>
            <ul id="nav-mobile" class="left hide-on-med-and-down">
                <li><a href="#">Our Vision</a></li>
                <li><a href="#">About Us</a></li>
                <li><a href="#">Contact</a></li>
            </ul>
        </div>
    </nav>
<div class="container">
  <div class="row">
    <div class="col s5"><span class="flow-text">
        <div id="current-doctors" class="current-patient ">
          <table>
            <tr>
              <th>Current Patients</th>
            </tr>
            {{#each doctors}}
            {{#if current_patient}}
            <tr>
              <td>{{this.patient_name}}</td>
            </tr>
            {{/if}}
            {{/each}}
          </table>
        </div>
      </span>
    </div>
    <div class="col s6"><span class="flow-text">
        <div id="new-patient-request" class="new-patient-request ">
          <table>
            <tr>
              <th>New Patients Requests</th>
            </tr>
            {{#each doctors}}
            {{#if patient_request}}
            <tr id="patient{{patientId}}">
              <td>{{this.patient_name}}
                  <a class="accept-patient btn-small float-right ml-2" name="accept" data-patientId={{this.patientId}} data-doctorId={{this.doctorId}}>
                    <i  class="material-icons add_box text-center" >done</i></a>
                <a class="decline-patient btn-small float-right ml-2" name="decline" data-patientId={{this.patientId}} data-doctorId={{this.doctorId}}>
                 <i  class="material-icons add_box text-center" >close</i></a>
                </td>
            </tr>
            {{/if}}

            {{/each}}
          </table>
        </div>
      </span>
    </div>
  </div>
</div>


