
<div class="container">
  <div class="row">
    <div class="col s5"><span class="flow-text">
        <div id="current-doctors" class="current-patient ">
          <table>
            <tr>
              <th>Doctor Information</th>
            </tr>
           
            {{#each doctors}}
             <tr>
              <td><strong class="">Doctor's Name:   </strong> {{this.doctor_name}}</td>
              </tr>
              <tr>
              <td><strong>Doctor's Primary Address:</strong> {{this.doctor_primary_address1}}</td>
              </tr>
              <tr>
              <td><strong>Doctor's Specilization: </strong>{{this.doctor_specilization}}</td>
              </tr>
            {{/each}}
            
          </table>
        </div>
      </span>
    </div>
  </div>
</div>


